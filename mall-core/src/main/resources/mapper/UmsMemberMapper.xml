<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wxq.mall.mapper.UmsMemberMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="baseMap" type="com.wxq.mall.model.UmsMember">
        <id column="id" property="id" />
        <result column="username" property="username" />
        <result column="password" property="password" />
        <result column="nickname" property="nickname" />
        <result column="head_img" property="headImg" />
        <result column="register_method" property="registerMethod" />
        <result column="phone" property="phone" />
        <result column="email" property="email" />
        <result column="status" property="status" />
        <result column="create_time" property="createTime" />
        <result column="login_time" property="loginTime" />
    </resultMap>


    <select id="findByPage" resultMap="baseMap">
        select id,username,password,nickname,head_img,register_method,phone,email,status,create_time,login_time
        from ums_member
    </select>

    <select id="findAll" resultMap="baseMap">
        select id,username,password,nickname,head_img,register_method,phone,email,status,create_time,login_time
        from ums_member
    </select>
</mapper>
