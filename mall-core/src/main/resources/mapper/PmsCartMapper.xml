<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wxq.mall.mapper.PmsCartMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="baseMap" type="com.wxq.mall.model.PmsCart">
        <id column="id" property="id" />
        <result column="account" property="account" />
        <result column="product_id" property="productId" />
        <result column="product_name" property="productName" />
        <result column="sku_code" property="skuCode" />
        <result column="product_attr" property="productAttr" />
        <result column="price" property="price" />
        <result column="quantity" property="quantity" />
        <result column="picture" property="picture" />
        <result column="business_code" property="businessCode" />
        <result column="business_name" property="businessName" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>


    <select id="findByPage" resultMap="baseMap">
        select id,account,product_id,product_name,sku_code,product_attr,price,quantity,picture,business_code,business_name,create_time,update_time
        from pms_cart
    </select>

    <select id="findAll" resultMap="baseMap">
        select id,account,product_id,product_name,sku_code,product_attr,price,quantity,picture,business_code,business_name,create_time,update_time
        from pms_cart
    </select>
</mapper>
