<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wxq.mall.mapper.OmsOrderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="baseMap" type="com.wxq.mall.model.OmsOrder">
        <id column="id" property="id" />
        <result column="account" property="account" />
        <result column="order_code" property="orderCode" />
        <result column="total_amount" property="totalAmount" />
        <result column="pay_amount" property="payAmount" />
        <result column="pay_type" property="payType" />
        <result column="souce_type" property="souceType" />
        <result column="stauts" property="stauts" />
        <result column="order_type" property="orderType" />
        <result column="confirm_status" property="confirmStatus" />
        <result column="create_time" property="createTime" />
    </resultMap>


    <select id="findByPage" resultMap="baseMap">
        select id,account,order_code,total_amount,pay_amount,pay_type,souce_type,stauts,order_type,confirm_status,create_time
        from oms_order
    </select>

    <select id="findAll" resultMap="baseMap">
        select id,account,order_code,total_amount,pay_amount,pay_type,souce_type,stauts,order_type,confirm_status,create_time
        from oms_order
    </select>
</mapper>
